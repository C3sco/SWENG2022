<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:gwt = 'urn:import:com.google.gwt.user.client.ui'>
	<ui:style>
		#navbar{
		  list-style-type: none;
		  margin: 0;
		  padding: 0;
		  overflow: hidden;
		  background-color: #303030;
		}
		
		li {
		  float: left;
		  
		}
		
		#Register{
			width: 50vw;
			margin-top: 10vh;
			margin-left: 30vw;
	
		}
	</ui:style>
	<g:HTMLPanel>
		<ul id="navbar">
	       <li  id="Home">
	       	<gwt:Button ui:field = "btnHome" text="Home"
                 />
	       </li>
	       <li  id="IscrizioneCorso">
	       	<gwt:Button ui:field = "btnIscrizione" text="Corsi Disponibili"
                 />
	       </li>
	        <li  id="RegistrazioneEsame">
	       	<gwt:Button ui:field = "btnRegistrazione" text="Registrazione Esame"
                 />
	       </li>
	        <li  id="ShowVoti">
	       	<gwt:Button ui:field = "btnVoti" text="Voti"
                   />
	       </li>
	         <li  id="Logout">
	       	<gwt:Button ui:field = "btnLogout" text="Logout"
                   />
	       </li>
	    </ul>
	    <div id="Register">
	      <div class="form-group">
		    <label for="exampleInputEmail1">Corso</label>
		    <gwt:MenuBar ui:field="menuCorsi"></gwt:MenuBar>
		  </div>
		  <div class="form-group">
		    <label for="exampleInputPassword1">Appelli</label>
		    <gwt:MenuBar ui:field="menuAppelli"></gwt:MenuBar>
		  </div>
		 
		  <gwt:Button ui:field="btnEsame" text="Iscrizione"></gwt:Button>
	    </div>
	</g:HTMLPanel>
</ui:UiBinder> 