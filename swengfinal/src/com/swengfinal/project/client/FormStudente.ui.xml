<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:gwt = 'urn:import:com.google.gwt.user.client.ui'>
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
		<form>
		  <div class="form-group">
		    <label for="exampleInputEmail1">Email address</label>
		    <gwt:TextBox ui:field="txtMail"></gwt:TextBox>
		  </div>
		  <div class="form-group">
		    <label for="exampleInputPassword1">Password</label>
		    <gwt:TextBox ui:field="txtPassword"></gwt:TextBox>
		  </div>
		  <div class="form-group">
		    <label for="exampleInputPassword1">Matricola</label>
		    <gwt:TextBox ui:field="txtMatricola"></gwt:TextBox>
		  </div>
		  <div class="form-group">
		    <label for="exampleInputPassword1">Nome</label>
		    <gwt:TextBox ui:field="txtNome"></gwt:TextBox>
		  </div>
		  <div class="form-group">
		    <label for="exampleInputPassword1">Cognome</label>
		    <gwt:TextBox ui:field="txtCognome"></gwt:TextBox>
		  </div>
		  <div class="form-group">
		    <label for="exampleInputPassword1">Luogo di Nascita</label>
		    <gwt:TextBox ui:field="txtLuogoNascita"></gwt:TextBox>
		  </div>
		  <div class="form-group">
		    <label for="exampleInputPassword1">Data di Nascita</label>
		    <gwt:TextBox ui:field="txtDataNascita"></gwt:TextBox>
		  </div>
		 
		  <gwt:Button ui:field="btnSubmit" text="Submit"></gwt:Button>
		</form>
	</g:HTMLPanel>
</ui:UiBinder> 