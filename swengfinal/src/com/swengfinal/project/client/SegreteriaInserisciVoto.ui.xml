<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:gwt = 'urn:import:com.google.gwt.user.client.ui'>
	<ui:style>
	#navbar{
		  list-style-type: none;
		  margin: 0;
		  padding: 0;
		  overflow: hidden;
		  background-color: #303030;
		}
		
		li {
		  float: left;
		  
		}
	</ui:style>
	<g:HTMLPanel>
		<ul id="navbar">
	       <li  id="Home">
	       	<gwt:Button ui:field = "btnHomeSegreteria" text="Home"
                 />
	       </li>
	       <li  id="Inserisci Voto">
	       	<gwt:Button ui:field = "btnInserisciVoto" text="Inserisci Voto"
                 />
	       </li>
	        <li  id="Pubblica Voto">
	       	<gwt:Button ui:field = "btnPubblicaVoto" text="Pubblica Voto"
                 />
	       </li>
	       <li  id="Logout">
	       	<gwt:Button ui:field = "btnLogout" text="Logout"
                   />
	       </li>
	    </ul>
	  
	</g:HTMLPanel>
</ui:UiBinder> 